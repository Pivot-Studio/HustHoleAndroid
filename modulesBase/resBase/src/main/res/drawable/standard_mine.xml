<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <solid android:color="@color/HH_Surface" />
            <stroke android:color="@color/HH_Surface" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/HH_Background" />
            <stroke android:color="@color/HH_Background" />
        </shape>
    </item>
</selector>