<?xml version="1.0" encoding="utf-8"?>
<!--分支-->
<!--点击时的绿色按钮-->
<shape
    xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 内部颜色 -->
    <solid
        android:color="?attr/colorPrimaryVariant" android:state_pressed="false"/>
    <!-- 边缘线条颜色 -->
    <stroke
        android:width="0dp"
        android:color="?attr/colorPrimaryVariant" />
    <!-- 圆角的幅度 -->
    <corners
        android:topLeftRadius="13dip"
        android:topRightRadius="13dip"
        android:bottomLeftRadius="13dip"
        android:bottomRightRadius="13dip"/>
</shape>